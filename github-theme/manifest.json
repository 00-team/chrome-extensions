{
    "name": "00 Team Github Theme",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Github Contributions Color",

    "icons": {
        "16": "images/00.png",
        "32": "images/00.png",
        "48": "images/00.png",
        "128": "images/00.png"
